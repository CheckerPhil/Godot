config_version=4

_global_script_classes=[ {
"base": "Reference",
"class": "Item",
"language": "GDScript",
"path": "res://Items/Item.gd"
} ]
_global_script_class_icons={
"Item": ""
}

[application]

config/name="Village Magic"
run/main_scene="res://UI/Menu/StartMenu.tscn"
config/use_custom_user_dir=true
boot_splash/image="res://icon.png"
config/icon="res://icon.png"

[autoload]

PlayerStats="*res://Player/PlayerStats.tscn"
PauseMenu="*res://UI/Menu/Pause.tscn"
Settings="*res://Settings.tscn"
Autoload="*res://autoload.gd"
Inventory="*res://UI/Inventory/Inventory.gd"

[display]

window/size/width=320
window/size/height=180
window/size/test_width=1280
window/size/test_height=720
mouse_cursor/custom_image="res://UI/Cursor.png"
window/stretch/mode="2d"
window/stretch/aspect="keep"

[global]

